{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2016",
    "loose": false,
    "externalHelpers": true
  },
  "env": {
    "targets": {
      "node": "14"
    },
    "mode": "entry",
    "coreJs": "3.22"
  }
}
