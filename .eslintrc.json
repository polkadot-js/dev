{
  "parser": "babel-eslint",
  "extends": [
    "semistandard",
    "standard-react",
    "standard-jsx",
    "plugin:flowtype/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "flowtype",
    "jest"
  ],
  "globals": {
    "WebAssembly": true
  },
  "env": {
    "browser": true,
    "jest/globals": true,
    "node": true
  }
}
