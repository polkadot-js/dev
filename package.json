{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/dev/issues",
  "homepage": "https://github.com/polkadot-js/dev#readme",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/polkadot-js/dev.git"
  },
  "sideEffects": false,
  "type": "module",
  "version": "0.69.5",
  "versions": {
    "git": "0.69.5",
    "npm": "0.69.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "polkadot-dev-build-ts --compiler babel",
    "build:extra": "(cd packages/dev && polkadot-dev-copy-dir config build/config && polkadot-dev-copy-dir node build/node && polkadot-dev-copy-dir scripts build/scripts)",
    "build:release": "polkadot-ci-ghact-build --skip-beta",
    "clean": "polkadot-dev-clean-build",
    "docs": "polkadot-dev-build-docs",
    "lint": "polkadot-dev-run-lint",
    "postinstall": "./packages/dev/scripts/polkadot-dev-yarn-only.mjs",
    "prettier": "./packages/dev/scripts/polkadot-dev-run-prettier.mjs",
    "test": "polkadot-dev-run-test --env browser"
  },
  "devDependencies": {
    "@polkadot/dev": "workspace:packages/dev",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/yargs": "^17.0.22"
  },
  "resolutions": {
    "typescript": "^4.9.5"
  }
}
