{
  "$schema": "https://json.schemastore.org/swcrc",
  "env": {
    "dynamicImport": true,
    "mode": "entry",
    "targets": {
      "node": "14"
    }
  },
  "exclude": [
    "\\.d\\.ts",
    "\\.manual\\.ts",
    "\\.spec\\.ts",
    "\\.spec\\.tsx",
    "\\.test\\.ts",
    "\\.test\\.tsx",
    "mod\\.ts"
  ],
  "jsc": {
    "externalHelpers": true,
    "loose": false,
    "parser": {
      "dynamicImport": true,
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2015"
  },
  "minify": false,
  "module": {
    "ignoreDynamic": true,
    "type": "commonjs"
  }
}
