{
  "$schema": "https://json.schemastore.org/swcrc",
  "env": {
    "dynamicImport": true,
    "mode": "entry",
    "targets": {
      "node": "12"
    }
  },
  "jsc": {
    "assumptions": {
      "noDocumentAll": true,
      "privateFieldsAsProperties": true,
      "setPublicClassFields": true
    },
    "experimental": {
      "keepImportAssertions": true
    },
    "externalHelpers": true,
    "loose": false,
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "target": "es2016",
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    }
  },
  "minify": false,
  "module": {
    "ignoreDynamic": true,
    "type": "commonjs"
  }
}
