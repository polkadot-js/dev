{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false,
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2016",
    "loose": false,
    "externalHelpers": true
  },
  "exclude": [
    ".d.ts",
    ".manual.ts",
    ".spec.ts",
    ".test.ts",
    "mod.ts"
  ],
  "env": {
    "targets": {
      "node": "14"
    },
    "mode": "entry"
  }
}
